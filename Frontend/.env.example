# Frontend Environment Variables
# Copy this file to .env and update the values

# API Configuration
# Backend API Base URL
VITE_API_BASE_URL=http://localhost:5000/api

# Razorpay Configuration (for payments)
# For Test Mode: Use keys starting with rzp_test_...
# For Live Mode: Use keys starting with rzp_live_...
VITE_RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxx

# Cloudinary Configuration (for file uploads - optional)
# Uncomment when implementing file uploads
# VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
# VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# App Configuration
# App Name (for display purposes)
VITE_APP_NAME=Appzeto

# Environment
# Set to 'development' or 'production'
# VITE_NODE_ENV=development

# Socket.io Configuration (if using real-time features)
# VITE_SOCKET_URL=http://localhost:5000

# Google Maps API Key (if using maps/location features)
# VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Feature Flags (optional - for enabling/disabling features)
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_ERROR_TRACKING=false

# Firebase Configuration (for Push Notifications)
# Get these values from Firebase Console > Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Firebase VAPID Key (for Web Push)
# Get from Firebase Console > Project Settings > Cloud Messaging > Web Push certificates
VITE_FIREBASE_VAPID_KEY=your_vapid_key_here
